{
  "name": "react_native_starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build-android": "npm run bundle-android && cd android && ./gradlew assemblerelease && cd app/build/outputs/apk/release",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle-ios": "react-native bundle --entry-file ./index.js --platform ios --bundle-output ios/main.jsbundle --assets-dest ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "start": "npx react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.14.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/push-notification-ios": "1.8.0",
    "@react-native-firebase/app": "11.0.0",
    "@react-native-firebase/messaging": "11.0.0",
    "@react-navigation/native": "5.9.2",
    "@react-navigation/stack": "5.14.2",
    "@types/react-native-push-notification": "7.0.1",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "1.10.3",
    "react-native-push-notification": "7.2.2",
    "react-native-reanimated": "1.13.2",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "2.18.1",
    "react-native-toast-message": "1.4.9"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/runtime": "7.8.4",
    "@react-native-community/eslint-config": "1.1.0",
    "@types/jest": "25.2.3",
    "@types/react-native": "0.63.51",
    "@types/react-test-renderer": "16.9.2",
    "babel-jest": "25.1.0",
    "eslint": "7.21.0",
    "jest": "25.1.0",
    "metro-react-native-babel-preset": "0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "4.2.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
